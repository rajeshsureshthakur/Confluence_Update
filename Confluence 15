SELECT 
    "Application",
    "Value" as "CPU Value",
    'Warning' as "Status"
FROM T1 
WHERE "Metric" LIKE 'JVM%CPU%'

UNION ALL

SELECT 
    "Application",
    "Value",
    'Warning'
FROM T2
WHERE "Metric" LIKE 'JVM%CPU%'

UNION ALL

SELECT 
    "Application",
    "Value",
    'Warning'
FROM T3
WHERE "Metric" LIKE 'JVM%CPU%'

UNION ALL

SELECT 
    "Application",
    "Value",
    'Warning'
FROM T4
WHERE "Metric" LIKE 'JVM%CPU%'
